<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context=".Zuoye6Activity">

    <Button
            android:id="@+id/startService"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:text="启动服务"
            tools:ignore="MissingConstraints"/>
    <Button
            android:id="@+id/stopService"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:text="停止服务"
            tools:ignore="MissingConstraints" tools:layout_editor_absoluteY="64dp"
            tools:layout_editor_absoluteX="-16dp"/>
    <Button
            android:id="@+id/useService"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:text="使用服务"
            tools:ignore="MissingConstraints" tools:layout_editor_absoluteY="134dp"
            tools:layout_editor_absoluteX="0dp"/>
    <EditText
            android:id="@+id/input"
            android:hint="输入要计算的数字"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    <Button
            android:id="@+id/calSushu"
            android:text="计算素数"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

</LinearLayout>